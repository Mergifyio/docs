---
import { Content, frontmatter, getHeadings } from '../content/docs/index.mdx';
import MainLayout from '../layouts/MainLayout.astro';
---

<MainLayout
	content={{
		title: frontmatter.title,
		description: frontmatter.description,
		suppressTitle: frontmatter.suppressTitle,
	}}
	headings={getHeadings()}
>
	<Content />
</MainLayout>
